{
  "name": "asset-apps-web",
  "version": "1.0.0",
  "description": "Vue.js based web application for Assets",
  "license": "ISC",
  "author": "Michael Johnson",
  "scripts": {
    "start": "vue-cli-service serve",
    "build:web": "vue-cli-service build",
    "build:docker": "vue-cli-service build --no-unsafe-inline --dest ../app/dist/web",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vueuse/sound": "^1.1.7",
    "axios": "^0.21.4",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-barcode-reader": "0.0.3",
    "vue-currency-input": "^1.22.6",
    "vue-router": "^3.2.0",
    "vuetify": "^2.5.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
